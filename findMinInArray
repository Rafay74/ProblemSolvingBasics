public class FindMin {
    public static void main(String[] args) {
        int[] arr = { 23, 4,5 , 45, 76, 8,3 ,234, 546 ,6 ,-6};

        int ans2 = findmin(arr);
        System.out.println(ans2);
    }

    static int findmin(int[] arr ) {
        int min = arr[0];
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] < min) {
                min = arr[i];
            }
        }
        return min;
    }
}
