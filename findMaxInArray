public class FindMMax {
    public static void main(String[] args) {
        int[] arr = { 23, 4,5 , 45, 76, 8,3 ,234, 546 ,6 ,-6};

        int ans1 = findmax(arr);
        System.out.println(ans1);

    }

    static int findmax(int[] arr) {
        int max = arr[0];
        for (int i = 1; i < arr.length; i++) {
            if ( arr[i] > max) {
                max = arr[i];
            }

        }
        return max;
    }   
}
