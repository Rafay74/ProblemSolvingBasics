//How do u count the number of digits in any given number?

public class count {
    public static void main(String[] args) {
        int target = 456;
        int ans = count(target);
        System.out.print(ans);
    }

    static int count(int n ) {
        int count = 0;
        while ( n > 0) {
            count ++ ;
            n = n /10;
        }
        return count;
    }
}


//this will return 3.
